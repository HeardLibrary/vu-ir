{
  "name": "ngx-matomo-client",
  "version": "6.4.1",
  "author": "Emmanuel Roux (https://github.com/EmmanuelRoux)",
  "description": "Matomo (fka. Piwik) client for Angular applications",
  "keywords": [
    "matomo",
    "piwik",
    "angular",
    "analytics"
  ],
  "peerDependencies": {
    "@angular/common": "^17.0.0 || ^18.0.0",
    "@angular/core": "^17.0.0 || ^18.0.0"
  },
  "dependencies": {
    "tslib": "^2.3.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/EmmanuelRoux/ngx-matomo-client.git",
    "directory": "projects/tracker"
  },
  "bugs": {
    "url": "https://github.com/EmmanuelRoux/ngx-matomo-client/issues"
  },
  "license": "MIT",
  "homepage": "https://github.com/EmmanuelRoux/ngx-matomo-client#readme",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "schematics": "./schematics/collection.json",
  "ng-add": {
    "save": true
  },
  "sideEffects": false,
  "module": "fesm2022/ngx-matomo-client.mjs",
  "typings": "index.d.ts",
  "exports": {
    "./package.json": {
      "default": "./package.json"
    },
    ".": {
      "types": "./index.d.ts",
      "esm2022": "./esm2022/ngx-matomo-client.mjs",
      "esm": "./esm2022/ngx-matomo-client.mjs",
      "default": "./fesm2022/ngx-matomo-client.mjs"
    },
    "./core": {
      "types": "./core/index.d.ts",
      "esm2022": "./esm2022/core/ngx-matomo-client-core.mjs",
      "esm": "./esm2022/core/ngx-matomo-client-core.mjs",
      "default": "./fesm2022/ngx-matomo-client-core.mjs"
    },
    "./form-analytics": {
      "types": "./form-analytics/index.d.ts",
      "esm2022": "./esm2022/form-analytics/ngx-matomo-client-form-analytics.mjs",
      "esm": "./esm2022/form-analytics/ngx-matomo-client-form-analytics.mjs",
      "default": "./fesm2022/ngx-matomo-client-form-analytics.mjs"
    },
    "./router": {
      "types": "./router/index.d.ts",
      "esm2022": "./esm2022/router/ngx-matomo-client-router.mjs",
      "esm": "./esm2022/router/ngx-matomo-client-router.mjs",
      "default": "./fesm2022/ngx-matomo-client-router.mjs"
    },
    "./testing": {
      "types": "./testing/index.d.ts",
      "esm2022": "./esm2022/testing/ngx-matomo-client-testing.mjs",
      "esm": "./esm2022/testing/ngx-matomo-client-testing.mjs",
      "default": "./fesm2022/ngx-matomo-client-testing.mjs"
    }
  }
}
